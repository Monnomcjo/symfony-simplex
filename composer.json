{
    "name": "johanpresent/clean-architecture-ddd-example",
    "description": "DDD & Clean Architecture example in PHP",
    "keywords": ["architecture"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.2",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "ExampleApp\\": "ExampleApp/",
            "ExampleAppTest\\": "Tests/"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "ExampleApp\\": "ExampleApp/",
          "ExampleAppTest\\": "Tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
