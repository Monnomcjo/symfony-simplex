{
    "require": {
        "symfony/config": "^5.2",
        "symfony/dependency-injection": "^5.2",
        "symfony/dotenv": "^5.2",
        "symfony/http-foundation": "^5.2",
        "symfony/http-kernel": "^5.2",
        "symfony/orm-pack": "^2.1",
        "symfony/routing": "^5.2",
        "symfony/yaml": "^5.2"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    }
}
