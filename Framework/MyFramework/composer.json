{
    "require": {
        "doctrine/dbal": "^3.0",
        "illuminate/database": "^8.29",
        "symfony/dependency-injection": "^5.2",
        "symfony/http-foundation": "^5.2",
        "symfony/http-kernel": "^5.2",
        "symfony/routing": "^5.2",
        "twig/twig": "^3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "": "src/",
            "ExampleApp\\": "../../ExampleApp/",
            "Infrastructure\\": "../../Infrastructure/MyFrameWork/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    }
}
